{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react"],
  "rules": {
    "no-plusplus": "off",
    "no-console": "warn",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-props-no-spreading": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
